<div class="match-wrapper">
  <h2><%= match.title %></h2>
  <div class="match-info">
    <span class="label">Price:</span>
    <%= match.price %>
  </div>
  <div class="remove-mark">
    <label for="<%= match.id %>">Mark to remove</label>
    <input
      type="checkbox"
      id="<%= match.id %>"
      value="<%= match.id %>"
      name = "matchesToRemove"
    />
  </div>
</div>
